                      </header>
                      <section>
                        <table>
                            <tr>
                              <th>Title</th><th>Events</th>
                            </tr>
                          
                            <% for (var i = 0; i < data.length; i++) { %>
                              <tr>
                                <td><%= data[i].title %></td>
                                <td><%= data[i].dataTitle %></td>
                              </tr>    
                            <% } %>
                          </table>
                      </section>
                  </div>
              </div>
          </article>
  
      </div>
  
      <div id="menu"></div>
  
